{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start:dev": "pnpm d:up:dev && nest start --watch",
    "start:prod": "pnpm db:m:prod && node dist/main",
    "build": "pnpm db:g && nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:e2e": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" DOTENV_CONFIG_PATH=.env.test jest --runInBand --config ./test/jest-e2e.json",
    "test:e2e:ci": "pnpm d:up:test && pnpm db:m:test && pnpm test:e2e && pnpm d:down:test",
    "d:up:dev": "docker compose up -d",
    "d:down:dev": "docker compose down",
    "d:up:test": "docker compose -f docker-compose.test.yml up -d",
    "d:down:test": "docker compose -f docker-compose.test.yml down",
    "d:build:prod": "docker build -t nest-starter:latest .",
    "d:up:prod": "docker compose --env-file .env.production -f docker-compose.prod.yml up -d --pull always",
    "d:down:prod": "docker compose -f docker-compose.prod.yml down",
    "db:m:dev": "pnpm prisma migrate dev && pnpm db:g",
    "db:m:test": "dotenv -e .env.test -- pnpm prisma migrate reset --force",
    "db:m:prod": "pnpm prisma migrate deploy",
    "db:g": "pnpm prisma generate"
  },
  "dependencies": {
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "cron": "4.3.5",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.1",
    "ipaddr.js": "^2.3.0",
    "nanoid": "^3.3.11",
    "nestjs-zod": "^5.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.3",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jest": "^29.12.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
