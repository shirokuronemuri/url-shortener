{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "start:dev": "pnpm d:up && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "pnpm db:g && nest build",
    "start:prod": "pnpm db:m:prod && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm d:up:test && pnpm db:m:test && NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" dotenv -e .env.test -- jest --runInBand --config ./test/jest-e2e.json && pnpm d:down:test",
    "d:up": "docker compose up -d",
    "d:down": "docker compose down",
    "d:up:test": "docker compose -f docker-compose.test.yml up -d",
    "d:down:test": "docker compose -f docker-compose.test.yml down",
    "d:build:prod": "docker build -t nest-starter:latest .",
    "d:up:prod": "docker compose --env-file .env.production -f docker-compose.prod.yml up -d --pull always",
    "d:down:prod": "docker compose -f docker-compose.prod.yml down",
    "d:logs": "docker compose logs -f",
    "d:restart": "pnpm d:down && pnpm d:up",
    "db:m:dev": "pnpm prisma migrate dev && pnpm db:g",
    "db:m:test": "dotenv -e .env.test -- pnpm prisma migrate reset --force",
    "db:m:prod": "pnpm prisma migrate deploy",
    "db:g": "pnpm prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "ipaddr.js": "^2.3.0",
    "nanoid": "^5.1.6",
    "nestjs-zod": "^5.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
