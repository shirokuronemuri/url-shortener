{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": "(.e2e-spec.ts$|.int-spec.ts$)",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": [
    "./test/setup.ts"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/dist/"
  ]
}
